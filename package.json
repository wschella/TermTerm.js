{
  "name": "termterm.js",
  "version": "0.0.2",
  "description": "Small utility that converts string representations of RDF terms to a JSON representation as specified by RDF.js",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "prepare": "npm run build",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wschella/TermTerm.js.git"
  },
  "keywords": [
    "RDF",
    "SPARQL",
    "utility"
  ],
  "author": "Wout Schellaert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wschella/TermTerm.js/issues"
  },
  "homepage": "https://github.com/wschella/TermTerm.js#readme",
  "devDependencies": {
    "@types/jest": "^21.1.2",
    "@types/node": "^8.0.44",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.2.2",
    "gulp-sourcemaps": "^2.6.1",
    "merge-stream": "^1.0.1",
    "jest": "^21.2.1",
    "ts-jest": "^21.1.2",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "rdf-data-model": "^1.0.0",
    "rdf-js": "^1.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
